skId}`));\n   308\t    if (!logFile) {\n   309\t      onEvent({\n   310\t        type: 'error',\n   311\t        taskId,\n   312\t        message: 'Log file not found'\n   313\t      });\n   314\t      return;\n   315\t    }\n   316\t    \n   317\t    const logPath = path.join(LOG_DIR, logFile);\n   318\t    \n   319\t    // Start monitoring\n   320\t    const cleanup = monitorLog(logPath, (event) => {\n   321\t      try {\n   322\t        const chunk = streamParser.eventToChunk(event);\n   323\t        onEvent({\n   324\t          type: 'autonomous-event',\n   325\t          taskId,\n   326\t          event: {\n   327\t            type: event.type,\n   328\t            tool: streamParser.isToolUseEvent(event) ? event.tool : undefined,\n   329\t            content: chunk?.content,\n   330\t            error: streamParser.isErrorEvent(event) ? event.error : undefined\n   331\t          }\n   332\t        });\n   333\t      } catch (err) {\n   334\t        logger.error('Error processing event:', err);\n   335\t      }\n   336\t    }, {\n   337\t      onError: (error) => {\n   338\t        onEvent({\n   339\t          type: 'error',\n   340\t          taskId,\n   341\t          message: error.message\n   342\t        });\n   343\t      }\n   344\t    });\n   345\t    \n   346\t    activeMonitors.set(taskId, cleanup);\n   347\t  }).catch(err => {\n   348\t    onEvent({\n   349\t      type: 'error',\n   350\t      taskId,\n   351\t      message: 'Failed to start monitoring'\n   352\t    });\n   353\t  });\n   354\t  \n   355\t  // Return cleanup function\n   356\t  return () => {\n   357\t    const cleanup = activeMonitors.get(taskId);\n   358\t    if (cleanup) {\n   359\t      cleanup();\n   360\t      activeMonitors.delete(taskId);\n   361\t    }\n   362\t  };\n   363\t}"}]},"session_id":"3f9d47d5-892a-4f95-9b81-1633ebcfbe18"}
