{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_016BE1ozwt1Xkwf5hfeGW6UJ","type":"tool_result","content":"     1\t/**\n     2\t * Autonomous Session API Handlers\n     3\t * Provides endpoints for monitoring autonomous task sessions\n     4\t */\n     5\timport { monitorLog, parseLogFile, streamParser } from 'channelcoder';\n     6\timport * as fs from 'fs/promises';\n     7\timport * as path from 'path';\n     8\timport { existsSync } from 'fs';\n     9\timport { logger } from '../src/observability/logger.js';\n    10\t\n    11\tconst PROJECT_ROOT = path.resolve(process.cwd(), '..');\n    12\tconst AUTONOMOUS_BASE_DIR = path.join(PROJECT_ROOT, '.tasks/.autonomous-sessions');\n    13\tconst LOG_DIR = path.join(AUTONOMOUS_BASE_DIR, 'logs');\n    14\t\n    15\tinterface SessionInfo {\n    16\t  sessionName: string;\n    17\t  taskId: string;\n    18\t  parentId?: string;\n    19\t  logFile: string;\n    20\t  startTime: string;\n    21\t  status: string;\n    22\t  pid?: number;\n    23\t}\n    24\t\n    25\tinterface SessionStats {\n    26\t  messages: number;\n    27\t  toolsUsed: string[];\n    28\t  totalCost?: number;\n    29\t  model?: string;\n    30\t  lastOutput?: string;\n    31\t  lastUpdate?: string;\n    32\t}\n    33\t\n    34\t// Active monitors\n    35\tconst activeMonitors = new Map<string, () => void>();\n    36\t\n    37\t/**\n    38\t * List all autonomous sessions\n    39\t */\n    40\texport async function handleAutonomousList() {\n    41\t  try {\n    42\t    const files = await fs.readdir(AUTONOMOUS_BASE_DIR).catch(() => []);\n    43\t    const infoFiles = files.filter(f => f.endsWith('.info.json'));\n    44\t    \n    45\t    const sessions = await Promise.all(\n    46\t      infoFiles.map(async (file) => {\n    47\t        try {\n    48\t          const content = await fs.readFile(path.join(AUTONOMOUS_BASE_DIR, file), 'utf-8');\n    49\t          const info = JSON.parse(content) as SessionInfo;\n    50\t          \n    51\t          // Simple status check - if no activity for 2 minutes, mark as completed\n    52\t          let currentStatus = info.status;\n    53\t          if (currentStatus === 'running' && existsSync(info.logFile)) {\n    54\t            const stats = await fs.stat(info.logFile);\n    55\t            const minutesAgo = (Date.now() - stats.mtime.getTime()) / 1000 / 60;\n    56\t            if (minutesAgo > 2) {\n    57\t              currentStatus = 'completed';\n    58\t            }\n    59\t          }\n    60\t          \n    61\t          \n    62\t          // Get basic stats from log file\n    63\t          let stats = null;\n    64\t          if (existsSync(info.logFile)) {\n    65\t            try {\n    66\t              const events = await parseLogFile(info.logFile);\n    67\t              const toolSet = new Set<string>();\n    68\t              let messages = 0;\n    69\t              let totalCost = undefined;\n    70\t              let lastOutput = '';\n    71\t              \n    72\t              for (const event of events) {\n    73\t                if (streamParser.isAssistantEvent(event)) {\n    74\t                  messages++;\n    75\t                }\n    76\t                if (streamParser.isToolUseEvent(event)) {\n    77\t                  toolSet.add(event.tool);\n    78\t                }\n    79\t                if (streamParser.isResultEvent(event)) {\n    80\t                  totalCost = event.total_cost;\n    81\t                }\n    82\t                const chunk = streamParser.eventToChunk(event);\n    83\t                if (chunk?.content && chunk.content.trim().length > 10) {\n    84\t                  lastOutput = chunk.content.trim().substring(0, 100);\n    85\t                }\n    86\t              }\n    87\t              \n    88\t              stats = {\n    89\t                messages,\n    90\t                toolsUsed: Array.from(toolSet),\n    91\t                totalCost,\n    92\t                lastOutput\n    93\t              };\n    94\t            } catch {\n    95\t              // Ignore parse errors\n    96\t            }\n    97\t          }\n    98\t          \n    99\t          return {\n   100\t            ...info,\n   101\t            status: currentStatus,\n   102\t            stats\n   103\t          };\n   104\t        } catch {\n   105\t          return null;\n   106\t        }\n   107\t      })\n   108\t    );\n   109\t    \n   110\t    return {\n   111\t      success: true,\n   112\t      data: sessions\n   113\t        .filter(Boolean)\n   114\t        .sort((a, b) => \n   115\t          new Date(b!.startTime).getTime() - new Date(a!.startTime).getTime()\n   116\t        )\n   117\t    };\n   118\t  } catch (error) {\n   119\t    logger.error('Failed to list autonomous sessions:', error);\n   120\t    return {\n   121\t      success: false,\n   122\t      error: 'Failed to list sessions'\n   123\t    };\n   124\t  }\n   125\t}\n   126\t\n   127\t/**\n   128\t * Get session details including parsed log stats\n   129\t */\n   130\texport async function handleAutonomousGet(taskId: string) {\n   131\t  try {\n   132\t    // Find session info\n   133\t    const files = await fs.readdir(AUTONOMOUS_BASE_DIR).catch(() => []);\n   134\t    const infoFile = files.find(f => f.includes(`task-${taskId}`) && f.endsWith('.info.json'));\n   135\t    \n   136\t    if (!infoFile) {\n   137\t      return {\n   138\t        success: false,\n   139\t        error: 'Session not found'\n   140\t      };\n   141\t    }\n   142\t    \n   143\t    const info = JSON.parse(\n   144\t      await fs.readFile(path.join(AUTONOMOUS_BASE_DIR, infoFile), 'utf-8')\n   145\t    ) as SessionInfo;\n   146\t    \n   147\t    \n   148\t    // Parse log file for stats\n   149\t    const stats: SessionStats = {\n   150\t      messages: 0,\n   151\t      toolsUsed: [],\n   152\t      lastUpdate: new Date().toISOString()\n   153\t    };\n   154\t    \n   155\t    if (existsSync(info.logFile)) {\n   156\t      try {\n   157\t        const events = await parseLogFile(info.logFile);\n   158\t        const toolSet = new Set<string>();\n   159\t        let lastMessageTime = null;\n   160\t        \n   161\t        for (const event of events) {\n   162\t          if (streamParser.isAssistantEvent(event)) {\n   163\t            stats.messages++;\n   164\t            lastMessageTime = new Date();\n   165\t            if (!stats.model) {\n   166\t              stats.model = event.message.model;\n   167\t            }\n   168\t          }\n   169\t          \n   170\t          if (streamParser.isToolUseEvent(event)) {\n   171\t            toolSet.add(event.tool);\n   172\t          }\n   173\t          \n   174\t          if (streamParser.isResultEvent(event)) {\n   175\t            stats.totalCost = event.total_cost;\n   176\t          }\n   177\t          \n   178\t          const chunk = streamParser.eventToChunk(event);\n   179\t          if (chunk?.content) {\n   180\t            const content = chunk.content.trim();\n   181\t            if (content.length > 10) {\n   182\t              stats.lastOutput = content.substring(0, 200);\n   183\t            }\n   184\t          }\n   185\t        }\n   186\t        \n   187\t        stats.toolsUsed = Array.from(toolSet);\n   188\t        if (lastMessageTime) {\n   189\t          stats.lastUpdate = lastMessageTime.toISOString();\n   190\t        }\n   191\t      } catch (err) {\n   192\t        logger.warn('Error parsing log file:', err);\n   193\t      }\n   194\t    }\n   195\t    \n   196\t    return {\n   197\t      success: true,\n   198\t      data: {\n   199\t        ...info,\n   200\t        stats\n   201\t      }\n   202\t    };\n   203\t  } catch (error) {\n   204\t    logger.error('Failed to get autonomous session:', error);\n   205\t    return {\n   206\t      success: false,\n   207\t      error: 'Failed to get session details'\n   208\t    };\n   209\t  }\n   210\t}\n   211\t\n   212\t/**\n   213\t * Get combined log entries from all sessions\n   214\t */\n   215\texport async function handleAutonomousLogs(limit = 50) {\n   216\t  try {\n   217\t    const logEntries: any[] = [];\n   218\t    \n   219\t    // Get all log files\n   220\t    const logFiles = await fs.readdir(LOG_DIR).catch(() => {\n   221\t      logger.warn(`Log directory not found: ${LOG_DIR}`);\n   222\t      return [];\n   223\t    });\n   224\t    \n   225\t    // Sort log files by modification time (newest first)\n   226\t    const sortedFiles = await Promise.all(\n   227\t      logFiles.map(async (file) => {\n   228\t        const filePath = path.join(LOG_DIR, file);\n   229\t        const stats = await fs.stat(filePath);\n   230\t        return { file, mtime: stats.mtime.getTime() };\n   231\t      })\n   232\t    );\n   233\t    sortedFiles.sort((a, b) => b.mtime - a.mtime);\n   234\t    \n   235\t    // Parse recent log files (up to 10 most recent)\n   236\t    for (const { file: logFile } of sortedFiles.slice(0, 10)) {\n   237\t      const taskId = logFile.match(/task-([^-]+)/)?.[1] || 'unknown';\n   238\t      const logPath = path.join(LOG_DIR, logFile);\n   239\t      \n   240\t      if (existsSync(logPath)) {\n   241\t        try {\n   242\t          const events = await parseLogFile(logPath);\n   243\t          \n   244\t          // Get more events from each file (last 20 instead of 10)\n   245\t          for (const event of events.slice(-20)) {\n   246\t            const chunk = streamParser.eventToChunk(event);\n   247\t            if (chunk?.content) {\n   248\t              const content = chunk.content.trim();\n   249\t              if (content.length > 5) {\n   250\t                // Use event timestamp if available, otherwise use current time\n   251\t                const timestamp = event.timestamp || new Date().toISOString();\n   252\t                \n   253\t                logEntries.push({\n   254\t                  timestamp,\n   255\t                  taskId,\n